{
  "apollo": {
    "client": {
      "ssrForceFetchDelay": 100,
      "ssrMode": true
    },
    "network": {
      "batchInterval": 10,
      "opts": {
        "credentials": "same-origin"
      }
    }
  },
  "bodyParser": {
    "json": {},
    "raw": {},
    "text": {},
    "urlencoded": {
      "extended": false
    }
  },
  "graphql": {
    "local": true,
    "url": "/api/graphql"
  },
  "passport": {
    "local": {
      "saltlen": 64,
      "iterations": 1024,
      "digestAlgorithm": "sha512",
      "lastLoginField": "lastLoginAt"
    }
  },
  "session": {
    "name": "TRAM.session",
    "resave": false,
    "saveUninitialized": false,
    "store": {
      "ttl": 86400,
      "touchAfter": 1800
    }
  },
  "helmet": {
    "defaultTitle": "TRAM",
    "titleTemplate": "%s | TRAM"
  }
}
