{
  "name": "test",
  "version": "0.0.1",
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "if [ -z \"$npm_config_production\" ]; then typings install; fi",
    "build:client:vendor": "webpack --config webpack/client/vendor.js",
    "build:client": "webpack --config webpack/client/index.js",
    "build:server:vendor": "webpack --config webpack/server/vendor.js",
    "build:server": "webpack --config webpack/server/index.js",
    "build:vendor": "npm-run-all -p build:client:vendor build:server:vendor",
    "build:all": "npm-run-all build:vendor -p build:client build:server",
    "build": "npm-run-all -p build:client build:server",
    "start:client:hot": "node dist/server.hot.js",
    "start:server:hot:nodemon": "nodemon dist/server.js --watch dist/server.js",
    "start:server:hot:webpack": "webpack --config webpack/server/index.js --watch",
    "start:server:hot": "npm-run-all -p start:server:hot:nodemon start:server:hot:webpack",
    "start:server": "node dist/server.js",
    "start:hot": "npm-run-all -p start:client:hot start:server:hot",
    "start": "npm-run-all build start:hot",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean": "npm-run-all -p clean:dist clean:coverage",
    "test:phantom": "BROWSER=PhantomJS npm run test",
    "test:chrome": "BROWSER=Chrome npm run test",
    "test:firefox": "BROWSER=Firefox npm run test",
    "test": "npm-run karma start karma.conf.js",
    "coverage": "open coverage/html/index.html",
    "lint": "npm-run tslint \"src/**/**.ts*\" --force"
  },
  "dependencies": {
    "apollo-client": "^0.5.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "graphql": "^0.7.2",
    "graphql-server-express": "^0.4.3",
    "graphql-tag": "^0.1.15",
    "graphql-tools": "^0.8.0",
    "history": "^4.4.0",
    "lodash": "^4.16.6",
    "npm-run": "^4.1.0",
    "npm-run-all": "^3.1.1",
    "react": "^15.3.2",
    "react-apollo": "^0.5.16",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "rethinkdb": "^2.3.3",
    "thinky": "^2.3.7",
    "validator": "^6.1.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "isomorphic-style-loader": "^1.1.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.13",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "serve-favicon": "^2.3.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "tslint-react": "^1.1.0",
    "typescript": "^2.0.7",
    "typings": "^1.5.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-bundle-analyzer": "^1.4.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-hot-middleware": "^2.13.1",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "engines": {
    "node": "6.7.0"
  }
}
